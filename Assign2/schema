STUDENT
	Name		VARCHAR
	Roll		CHAR		PRIMARY KEY
	Dept		VARCHAR
	RID		INT		FOREIGN KEY(ROLE.RID)

ROLE
	RID		INT		PRIMARY KEY
	Rname		VARCHAR
	Description	VARCHAR

EVENT
	EID		INT		PRIMARY KEY
	EName		VARCHAR
	Date		Date
	Type		VARCHAR
	
VOLUNTEER
	Roll		CHAR		PRIMARY KEY
	Dept		VARCHAR

PARTICIPANT
	PID		INT		PRIMARY KEY
	Name		VARCHAR
	College_Name	VARCHAR
	College_Loc	TEXT

	
MANAGE
	Roll		CHAR
	EID		INT		FOREIGN KEY(EVENT.EID)

EVENT_PART
	EID		INT		FOREIGN KEY(EVENT.EID)
	PID		INT		FOREIGN KEY(PARTICIPANT.PID)
	
EVENT_VOL
	EID		INT		FOREIGN KEY(EVENT.EID)
	Roll		CHAR		FOREIGN KEY(VOLUNTEER.Roll)
